{
  "name": "ts-reflection",
  "version": "0.3.0",
  "description": "Type reflection utilities for TypeScript.",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janjakubnanista/ts-reflection.git"
  },
  "keywords": [
    "TypeScript",
    "type properties",
    "type keys",
    "enum values",
    "enum keys"
  ],
  "author": "Jan Jakub Nanista <jan.jakub.nanista@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janjakubnanista/ts-reflection/issues"
  },
  "homepage": "https://github.com/janjakubnanista/ts-reflection#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@types/node": "^14.0.18",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "@wessberg/rollup-plugin-ts": "^1.2.27",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "fast-check": "^1.25.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rollup": "^2.20.0",
    "rollup-plugin-copy": "^3.3.0",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {
    "typescript": ">=2.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.spec.ts"
    ]
  },
  "lint-staged": {
    "*.{js,ts}(x)?": [
      "eslint --fix"
    ]
  }
}
